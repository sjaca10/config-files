<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://ww.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>org.postgres.postgres</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/pgsql/bin/postgres</string>
        <string>-D</string>
        <string>/usr/local/pgsql/data/</string>
        <string>-r</string>
        <string>/usr/local/pgsql/logs/output.log</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
<key>UserName</key>
<string>postgres</string>
<key>StandardErrorPath</key>
<string>/tmp/org.postgresql.initdb.error</string>
<key>StandardOutPath</key>
<string>/tmp/org.postgresql.initdb.log</string>
</dict>
</plist>